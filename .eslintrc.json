{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2019,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [0],
        "import/no-named-as-default": [0],
        "import/no-named-as-default-member": [0],
        "max-len": [0],
        "react/jsx-props-no-spreading": [0],
        "no-param-reassign": 0
    }
}
